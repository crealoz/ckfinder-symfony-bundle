{
    "name": "ckfinder/ckfinder-symfony-bundle",
    "description": "CKFinder bundle for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "CKSource",
            "homepage": "http://cksource.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^5.*",
        "symfony/form": "^5.*",
        "symfony/console": "^5.*",
        "pimple/pimple": "~3.0",
        "monolog/monolog": "~1.4|~2.0",
        "league/flysystem": "^1.0.37",
        "league/flysystem-aws-s3-v3": "^1.0.13",
        "league/flysystem-cached-adapter": "^1.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "config": {
      "sort-packages": true,
      "preferred-install": {
        "symfony/doctrine-bridge": "source",
        "*": "dist"
      }
    },
    "autoload": {
        "psr-4": {
            "CKSource\\Bundle\\CKFinderBundle\\": "/src"
        }
    }
}
